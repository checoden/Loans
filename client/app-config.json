{
  "app": {
    "name": "Займы онлайн",
    "package": "ru.yourcompany.microloans",
    "versionName": "1.0.0",
    "versionCode": 1
  },
  "buildSettings": {
    "android": {
      "minSdkVersion": 21,
      "targetSdkVersion": 33,
      "compileSdkVersion": 33
    },
    "ios": {
      "deploymentTarget": "13.0"
    }
  },
  "android": {
    "permissions": [
      "android.permission.INTERNET",
      "android.permission.ACCESS_NETWORK_STATE",
      "android.permission.WAKE_LOCK",
      "android.permission.VIBRATE",
      "android.permission.RECEIVE_BOOT_COMPLETED"
    ]
  },
  "ios": {
    "bundleIdentifier": "ru.yourcompany.microloans",
    "capabilities": [
      "push-notification"
    ]
  },
  "oneSignal": {
    "appId": "${VITE_ONESIGNAL_APP_ID}"
  }
}